{
  "expo": {
    "name": "thirdweb playground",
    "slug": "hypx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.thirdweb.demo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thirdweb.demo",
      "associatedDomains": [
        "webcredentials:verify.hypx.xyz",
        "applinks:verify.hypx.xyz"
      ],
      "extraPods": [
        {
          "name": "OpenSSL-Universal",
          "version": "3.1.5004"
        }
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thirdweb.demo",
      "intentFilters": [
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "verify.hypx.xyz"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          },
          "ios": {
            "extraPods": [
              {
                "name": "OpenSSL-Universal",
                "configurations": [
                  "Release",
                  "Debug"
                ],
                "modular_headers": true,
                "version": "3.1.5004"
              }
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bfde95f1-07cf-4b7f-989a-c51e5681075a"
      }
    },
    "owner": "nanthandev"
  }
}
